function getTileDataForTile(tile)
    if tile.getVar("tileDataId") ~= nil then
        return globalTileData[tile.getVar("tileDataId")]
    else
        return {
            symmetry = tile.getVar("symmetry"),
            ignoredFeatures = tile.getTable("ignoredFeatures"),
            isStartingPiece = tile.getVar("isStartingPiece"),
            sides = tile.getTable("sides"),
            linkedQuadrants = tile.getTable("linkedQuadrants"),
            linkedOctants = tile.getTable("linkedOctants"),
            specialFeatures = tile.getTable("specialFeatures")
        }
    end
end
